<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kisa.kgh.kisa_fintech">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".funding_list.MyFundingListActivity" />
        <activity android:name=".funding_detail.FundingDetailActivity">
            <intent-filter>
                <action android:name="android.intent.action.RUN" />
            </intent-filter>
        </activity>
        <activity android:name=".new_funding.NewFundingActivity" />
        <activity android:name=".settings.SettingActivity" />
        <activity android:name=".funding_detail.PaymentGuideActivity" />
        <activity android:name=".funding_detail.SelectRewardActivity" />
        <activity android:name=".funding_detail.FundingPaymentActivity"></activity>
    </application>

</manifest>